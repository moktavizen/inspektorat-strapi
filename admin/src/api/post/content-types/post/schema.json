{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "nama": {
      "type": "string",
      "default": "Admin Inspektorat"
    },
    "email": {
      "type": "string",
      "default": "admin.inspektorat@gmail.com"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    }
  }
}
